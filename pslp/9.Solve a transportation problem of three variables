// Define the supply, demand, and cost matrices
supply = [10; 20; 30];
demand = [15 25 20];
cost = [2 3 4; 5 2 1; 3 4 2];

// Solve the transportation problem
[total_cost, transportation] = transportation_simplex(cost, supply, demand);

// Display the total cost
disp("Total cost: " + string(total_cost));

// Display the transportation matrix
disp("Transportation matrix:");
disp(transportation);